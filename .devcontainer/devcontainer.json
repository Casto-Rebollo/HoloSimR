{
  "name": "R Dev Container",
  "image": "rocker/rstudio:latest",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/common-utils:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "REditorSupport.r",
        "Ikuyadeu.r",
        "reditorsupport.r-lsp"
